<!-- breadcrumb -->
<div class="container">
	<div class="bread-crumb flex-w p-l-25 p-r-15 p-t-30 p-lr-0-lg">
		<a href="/" class="stext-109 cl8 hov-cl1 trans-04">
			Home
			<i class="fa fa-angle-right m-l-9 m-r-10" aria-hidden="true"></i>
		</a>
		<span class="stext-109 cl4">
			My orders
		</span>
	</div>
</div>


<!-- My orders -->
<form class="bg0 p-t-75 p-b-85">
	<div class="container">
		<div class="row">
			<div class="col-lg-10 col-xl-7 m-lr-auto m-b-50">
				<div class="m-l-25 m-r--38 m-lr-0-xl">
					<div class="wrap-table-shopping-cart">
						<table class="table-shopping-cart">
							<tr class="table_head">
								<th class="column-1">Order date</th>
								<th class="column-2">No of products</th>
								<th class="column-3">Amount</th>
								<th class="column-4">Payment mode</th>
								<th class="column-5">Status</th>
								<th class="column-6"></th>
							</tr>
							{{#each orders}}
							<tr class="table_row">
								<td class="column-1 ">{{this.date}}</td>
								<td class="column-2 pl-5">{{arrayLen this.products}}</td>
								<td class="column-3">â‚¹ {{this.net}}</td>
								<td class="column-4 pr-3">{{this.paymentMethod}}</td>
								{{#if (eq this.orderStatus "cancelled")}}
								<td class="column-5" style="color: red" ;>Cancelled</td>
								{{else}}
								<td class="column-5">{{this.paymentStatus}}</td>
								{{/if}}
								<td class="column-6"></td>
							</tr>
							{{/each}}
						</table>
					</div>


				</div>
			</div>


		</div>
	</div>
</form>




