<!-- breadcrumb -->
<div class="container">
	<div class="bread-crumb flex-w p-l-25 p-r-15 p-t-30 p-lr-0-lg">
		<a href="/" class="stext-109 cl8 hov-cl1 trans-04">
			Home
			<i class="fa fa-angle-right m-l-9 m-r-10" aria-hidden="true"></i>
		</a>

		<span class="stext-109 cl4">
			wishlist
		</span>
	</div>
</div>

<!-- wishlist -->
<div class="d-flex justify-content-center">
	<h4>Wishlist</h4>
</div>
<div class="bg0 p-t-75 p-b-85">
	<div class="container">
		<div class="row">
			<div class="col-lg-10 col-xl-7 m-lr-auto m-b-50">
				<div class="m-l-25 m-r--38 m-lr-0-xl">
					<div id="wish-list" class="wrap-table-shopping-cart">

						<table class="table-shopping-cart">
							<tr class="table_head">
								<th class="column-1">Product</th>
								<th class="column-2"></th>
								<th class="column-3 px-4">Price</th>
								{{!-- <th class="column-4"></th> --}}
								<th class="column-5 px-4"></th>
								<th class="column-6"></th>
							</tr>
							{{#each data}}
							<tr class="table_row">
								<td class="column-1">
									<div class="how-itemcart1">
										<img src="productsImages/{{this.product.image.[0]}}" alt="IMG">
									</div>
								</td>
								<td class="column-2">{{this.product.name}}</td>
								<td class="column-3 px-3">â‚¹ {{this.product.price}}</td>

								<td class="column-5 px-4"><button onclick="addToCart('{{this.product._id}}')">
										<div
											class="flex-c-m stext-101 cl2 size-118 bg8 bor13 hov-btn3 p-lr-15 trans-04 pointer">
											Add to Cart
										</div>
									</button></td>
								<td class="column-6"><button onclick="deleteFromWishlist('{{this.product._id}}')"
										class="btn-danger rounded-pill px-4 py-2 mr-3 ml-2">
										Remove
									</button></td>
							</tr>
							{{/each}}

						</table>
					</div>

				</div>
			</div>

		</div>
	</div>
</div>

