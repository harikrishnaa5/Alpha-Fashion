<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <title>DarkPan - Bootstrap 5 Admin Template</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta content="" name="keywords">
    <meta content="" name="description">


    <!-- Fontawesome -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.2.0/css/all.min.css">

    <!-- Favicon -->
    <link href="/admin-files/img/favicon.ico" rel="icon">

    <!-- Google Web Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@400;600&family=Roboto:wght@500;700&display=swap"
        rel="stylesheet">

    <!-- Icon Font Stylesheet -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.1/font/bootstrap-icons.css" rel="stylesheet">

    <!-- Libraries Stylesheet -->
    <link href="lib/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet">
    <link href="lib/tempusdominus/css/tempusdominus-bootstrap-4.min.css" rel="stylesheet" />

    <!-- Customized Bootstrap Stylesheet -->
    <link href="/admin-files/css/bootstrap.min.css" rel="stylesheet">

    <!-- Template Stylesheet -->
    <link href="/admin-files/css/style.css" rel="stylesheet">
</head>

<body>
    <div class="container-fluid position-relative d-flex p-0">
        <!-- Spinner Start -->
        <div id="spinner"
            class="show bg-dark position-fixed translate-middle w-100 vh-100 top-50 start-50 d-flex align-items-center justify-content-center">
            <div class="spinner-border text-primary" style="width: 3rem; height: 3rem;" role="status">
                <span class="sr-only">Loading...</span>
            </div>
        </div>
        <!-- Spinner End -->


        <!-- Sidebar Start -->
        <div class="sidebar pe-4 pb-3">
            <nav class="navbar bg-secondary navbar-dark">
                <a href="/admin" class="navbar-brand mx-4 mb-3">
                    <h5 class="text-success"><i class="fa fa-user-edit me-2"></i>Alpha Fashion</h5>
                </a>
                <div class="d-flex align-items-center ms-4 mb-4">
                    <div class="position-relative">
                        <img class="rounded-circle" src="/admin-files/img/user.jpg" alt=""
                            style="width: 40px; height: 40px;">
                        <div
                            class="bg-success rounded-circle border border-2 border-white position-absolute end-0 bottom-0 p-1">
                        </div>
                    </div>
                    <div class="ms-3">
                        <h6 class="mb-0">Harikrishna N</h6>
                        <span>Admin</span>
                    </div>
                </div>
                <div class="navbar-nav w-100">
                    <a href="/admin" class="nav-item nav-link"><i
                            class="fa fa-tachometer-alt me-2"></i>Dashboard</a>
                    <a href="/admin/user-list" class="nav-item nav-link"><i class="fa fa-th me-2"></i>User</a>
                    <a href="/admin/orders" class="nav-item nav-link"><i class="fa fa-th me-2"></i>Orders</a>
                    <a href="/admin/category" class="nav-item nav-link"><i class="fa fa-keyboard me-2"></i>Category</a>
                    <a href="/admin/products" class="nav-item nav-link"><i class="fa fa-table me-2"></i>Product</a>
                    <a href="/admin/banner" class="nav-item nav-link"><i class="fa fa-chart-bar me-2"></i>Banner</a>
                    <a href="/admin/coupon" class="nav-item nav-link" ><i class="far fa-file-alt me-2"></i>Coupon</a>

            
                      <a href="/admin/logout" class="nav-item nav-link"><i class="fa fa-table me-2"></i>Logout</a>
                </div>
            </nav>
        </div>
        <!-- Sidebar End -->


<div class="content pl-5 pt-5">
      <div class="row tm-content-row">
        <div class="col-12 ml-5 mt-5">
          <div class="tm-bg-primary-dark tm-block tm-block-products ml-5">
            <div id="userTable" style="min-height: 500px; width : 85%" class="tm-product-table-container">
              <div class="card" style="background-color: #a1abb5;">
                        <div id="" class="card-body p-4">
                            <div class="d-flex justify-content-between align-items-center">
                                <p class="small text-white mb-0">Order ID : {{order._id}}</p>
                                <h2 class="text-white text-uppercase">{{order.orderStatus}}</h2>
                            </div>
                            <div class="d-flex justify-content-between align-items-center mb-4">
                                <p class="small text-white mb-0">Order date : {{order.date}}</p>
                            </div>
                            <div>
                                {{#each order.products}}
                                <div style="background-color:#62727e;" class=" card shadow-0 border mb-4">
                                    <div class="card-body">
                                        <div class="row">
                                            <div class="col-md-2">
                                                <img src="/productsImages/{{this.product.image.[0]}}"
                                                    class="img-fluid" alt="Phone">
                                            </div>
                                            <div class="col-md-6 d-flex align-items-center">
                                                <p class="text-white mb-0">{{this.product.name}}</p>
                                            </div>
                                            <div class="col-md-2 d-flex align-items-center">
                                                <p class="text-white mb-0 small">Qty: {{this.quantity}}</p>
                                            </div>
                                            <div class="col-md-2 d-flex align-items-center">
                                                <p class="text-white mb-0 small">₹ {{this.pricePerItem}}</p>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                {{/each}}
                            </div>
                            {{!-- {{/each}} --}}

                            <div class="d-flex justify-content-between pt-2">
                                <p class="text-white mb-0"><span class="fw-bold me-4"></span></p>
                                <p class="text-white mb-0"><span
                                        class="fw-bold me-4">Subtotal:</span>₹{{order.subtotal}}.00</p>
                            </div>

                            <div class="d-flex justify-content-between pt-2">
                                <p class="text-white mb-0"><span class="fw-bold me-4">Shipping address:</span></p>
                            </div>

                            <div class="d-flex justify-content-between  ">
                                <p class="text-white mb-0">{{address.name}}, {{address.house}},</p>
                                {{#if order.coupon}}
                                <p class="text-white mb-0"><span class="fw-bold me-4">Coupon used</span>{{order.coupon}}
                                </p>
                                {{/if}}
                            </div>

                            <div class="d-flex justify-content-between">
                                <p class="text-white mb-0">{{address.locality}}, {{address.area}},</p>
                                <p class="text-white mb-0"><span class="fw-bold me-4">Coupon discount</span>
                                    ₹{{order.discount}}.00</p>
                            </div>

                            <div class="d-flex justify-content-between mb-5">
                                <p class="text-white mb-0">{{address.landmark}}, {{address.pincode}}</p>
                                <p class="text-white mb-0"><span class="fw-bold me-4">Delivery
                                        Charges</span> ₹50.00</p>
                            </div>
                            <div class="d-flex justify-content-between mb-5">
                                <p class="text-white mb-0"></p>
                                <p class="small text-white mb-0">Payment mode: {{order.paymentMethod}}</p>
                            </div>
                        </div>
                        <div style="background-color:#435c70;" class="card-footer border-0 px-4 py-5"
                            style="background-color: #c5c5c5; border-bottom-left-radius: 10px; border-bottom-right-radius: 10px;">
                            <h5 class="d-flex align-items-center justify-content-end text-white text-uppercase mb-0">
                                Total
                                amount: <span class="h2 mb-0 ms-2">₹{{order.net}}.00</span></h5>
                        </div>
                    </div>
            </div>
            <!-- table container -->
          </div>
        </div>        
      </div>
    </div>

     <!-- JavaScript Libraries -->
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
    <script src="lib/chart/chart.min.js"></script>
    <script src="lib/easing/easing.min.js"></script>
    <script src="lib/waypoints/waypoints.min.js"></script>
    <script src="lib/owlcarousel/owl.carousel.min.js"></script>
    <script src="lib/tempusdominus/js/moment.min.js"></script>
    <script src="lib/tempusdominus/js/moment-timezone.min.js"></script>
    <script src="lib/tempusdominus/js/tempusdominus-bootstrap-4.min.js"></script>

    <!-- Template Javascript -->
    <script src="/admin-files/js/main.js"></script>

    <script>
        function index(num){
            return num1 + 1;
        }
    </script>


</body>

</html>